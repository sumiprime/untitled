    <section class="welcome">
      <h2 class="fontface"> <span class="green">Welcome</span>.This is our Website.Enjoy!</h2>





  </div>
  <!--end holder-->


  <!--start holder-->

  <div class="content1">

    <section class="news">
      <h3 class="fontface"> News</h3>
